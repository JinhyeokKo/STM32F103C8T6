#include "button_controller.h"
#include "oled_controller.h"

int count = 0;

void checkButton(){

//	  printf("push\r\n");

	  count++;
	  if(count > 99){
		  count = 0;
	  }
	  printTemper(count);

//	  printf("down\r\n");

	  count--;
	  if(count < 0){
		  count = 99;
	  }
	  printTemper(count);

//	  printf("fix\r\n");
